pipeline {
    agent any
    stages {
        stage( 'build') {
            steps {
                echo 'building..'
            }
        }

        stage( 'test' ) {
            steps {
                echo 'testing...'
            }
        }

        steps( 'deploy' ) {
            steps {
                echo 'deploying..'
            }
        }
    }
}
